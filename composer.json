{
    "name": "fioru/lasntgadmin-products",
    "version": "5.14.1",
    "type": "wordpress-plugin",
    "extra": {
        "installer-name": "lasntgadmin-products"
    },
    "autoload": {
        "psr-4": {
            "Lasntg\\Admin\\Products\\": "lib"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lasntg\\Admin\\Test\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit --verbose --configuration phpunit.xml.dist --prepend vendor/autoload.php --testdox --colors=always",
        "lint": "phplint ./ --exclude=vendor",
        "check": "phpcs -ns",
        "fix": "phpcbf",
        "secure": "security-checker security:check composer.lock",
        "all": ["@lint", "@check", "@test", "@secure"]
    },
    "repositories": [
        {
            "type": "vcs",
            "url":  "git@github.com:fioru-software/lasntgadmin-attendees.git"
        },
        {
            "type": "vcs",
            "url":  "git@github.com:fioru-software/lasntgadmin-itthinx-groups.git"
        },
        {
            "type": "vcs",
            "url":  "git@github.com:fioru-software/lasntgadmin-users.git"
        },
        {
            "type": "vcs",
            "url":  "git@github.com:fioru-software/lasntgadmin-subscriptions.git"
        },
        {
            "type": "vcs",
            "url":  "git@github.com:fioru-software/lasntgadmin-groups.git"
        },
        {
            "type": "vcs",
            "url":  "git@github.com:fioru-software/lasntgadmin-orders.git"
        },
        {
            "type": "vcs",
            "url":  "git@github.com:fioru-software/lasntgadmin-grant-funded-payment-gateway.git"
        },
        {
            "type": "vcs",
            "url":  "git@github.com:fioru-software/lasntgadmin-products.git"
        },
        {
            "type": "vcs",
            "url":  "git@github.com:fioru-software/lasntgadmin-translate.git"
        },
        {
            "type": "vcs",
            "url":  "git@github.com:fioru-software/lasntgadmin-theme.git"
        },
        {
            "type": "vcs",
            "url":  "git@github.com:fioru-software/lasntgadmin-dashboard.git"
        }
    ],
    "require": {
        "composer/installers": "~2.0",
        "fioru/lasntgadmin-grant-funded-payment-gateway": "^1.0.8",
        "fioru/lasntgadmin-attendees": "^3.7.11",
        "fioru/lasntgadmin-users": "^3.0.2",
        "fioru/lasntgadmin-orders": "^1.7.29",
        "fioru/lasntgadmin-groups": "^1.5.6",
        "fioru/lasntgadmin-itthinx-groups": "^2.19.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.19",
        "overtrue/phplint": "^3.0",
        "yoast/phpunit-polyfills": "^1.0",
        "mockery/mockery": "^1.5",
        "squizlabs/php_codesniffer": "^3.7",
        "phpcompatibility/phpcompatibility-wp": "*",
        "dealerdirect/phpcodesniffer-composer-installer": "^0.7",
        "phpcompatibility/php-compatibility": "^9.3",
        "wp-coding-standards/wpcs": "dev-develop",
        "enlightn/security-checker": "^1.11.0"
    },
    "minimum-stability": "dev",
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "composer/installers": true
        }
    }
}
