{
    "name": "fioru/lasntgadmin-products",
    "version": "5.3.0",
    "type": "wordpress-plugin",
    "extra": {
        "installer-name": "lasntgadmin-products"
    },
    "autoload": {
        "psr-4": {
            "Lasntg\\Admin\\Products\\": "lib"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lasntg\\Admin\\Test\\": "tests/"
        }
    },
    "scripts": {
      "test": "phpunit --verbose --configuration phpunit.xml.dist --prepend vendor/autoload.php --testdox --colors=always",
      "lint": "phplint ./ --exclude=vendor",
      "check": "phpcs -ns",
      "fix": "phpcbf",
      "secure": "security-checker security:check composer.lock",
      "all": [ "@lint", "@check", "@test", "@secure" ]
    },
    "repositories": [
        {
            "type": "vcs",
            "url":  "git@github.com:fioru-software/lasntgadmin-grant-funded-payment-gateway.git"
        },
        {
            "type": "vcs",
            "url":  "git@github.com:fioru-software/lasntgadmin-groups.git"
        }
    ],
    "require": {
        "composer/installers": "~2.0",
        "fioru/lasntgadmin-grant-funded-payment-gateway": "^1.0.7"
    },
    "require-dev": {
        "fakerphp/faker": "^1.19",
        "overtrue/phplint": "^3.0",
        "yoast/phpunit-polyfills": "^1.0",
        "mockery/mockery": "^1.5",
        "squizlabs/php_codesniffer": "^3.7",
        "phpcompatibility/phpcompatibility-wp": "*",
        "dealerdirect/phpcodesniffer-composer-installer": "^0.7",
        "phpcompatibility/php-compatibility": "^9.3",
        "wp-coding-standards/wpcs": "dev-develop",
        "enlightn/security-checker": "dev-main"
    },
    "minimum-stability": "dev",
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "composer/installers": true
        }
    }
}
